{
  "name": "aws-nextjs-app",
  "version": "0.1.0",
  "private": true,
  "browser": {
    "crypto": false
  },
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "test": "NODE_OPTIONS=--experimental-vm-modules vitest run",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules vitest run --coverage",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules vitest run --coverage ./__tests__/utils/auth.test.ts",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules vitest"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.958.0",
    "@aws-sdk/client-dynamodb": "^3.958.0",
    "@aws-sdk/lib-dynamodb": "^3.958.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.1.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.4",
    "eslint-plugin-react": "^7.33.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  }
}